{
    "editor.minimap.enabled": true,
  //  "editor.fontFamily": "'OperatorMono-Light', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "window.zoomLevel": 0,
    "editor.mouseWheelZoom": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.iconTheme": "material-icon-theme",

    "window.title": "${rootName}${separator}${activeEditorLong}",

    //"workbench.colorTheme": "Monokai",
    "workbench.statusBar.visible": true,
    "workbench.sideBar.location": "left",
    "workbench.activityBar.visible": true,

    "todohighlight.keywords": [
        "CHECK:",
        "TODO:",
        "REMOVE:",
        "TEST:"
    ],
    "todohighlight.isEnable": true,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.xml",
        "**/*.css",
        "**/*.scss"
    ],
    "material-icon-theme.folders.theme": "classic",
    "bookmarks.saveBookmarksInProject": true,
    "files.eol": "\n",
    "[markdown]": {
        "editor.quickSuggestions": true
    },
    "auto-close-tag.fullMode": true,
    "update.channel": "none",
    "bracketPairColorizer.consecutivePairColors": [
        "()",
        "[]",
        "{}",
        [
            // "Gold",
            "Orchid",
            "LightSkyBlue",
            "Lime"

        ],
        "Red"
    ],
    "workbench.fontAliasing": "auto",
    "gitlens.historyExplorer.enabled": false,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "vscode-json-editor.theme": "dark",
    // Wes Bos setting === begin ====
    "workbench.colorTheme": "Cobalt2",
    "editor.fontFamily": "OperatorMono-Light, Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 25,
    "editor.letterSpacing": 0.5,
    "files.trimTrailingWhitespace": true,
    "editor.fontWeight": "400",
    "prettier.eslintIntegration": true,
    "editor.cursorStyle": "line-thin",
    "editor.cursorWidth": 5,
    "editor.cursorBlinking": "solid",
    // Very important: Install this plugin: https://github.com/be5invis/vscode-custom-css
    // You'll need to change this to a file URI scheme on your computer: https://en.wikipedia.org/wiki/File_URI_scheme
    // Mac/Linux: file:///Users/YOUR-PC-USERNAME/.vscodestyles.css
    // Windows: file:///C:/Users/YOUR-PC-USERNAME/.vscodestyles.css
    "vscode_custom_css.imports": [
      "file://Users/TG000538/.vscodestyles.css"
    ],
    "editor.renderWhitespace": "all",
    "vscode_custom_css.policy": true,
    // Wes Bos setting === end ===
    "terminal.integrated.fontFamily": "Monaco Nerd Font",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.cursorStyle": "line"
}